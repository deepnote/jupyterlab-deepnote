    "bdist",
    "chakra",
    "codegen",
    "coreutils",
    "csstree",
    "deepnote",
    "exenv",
    "ipynb",
    "Jakubowski",
    "jlpm",
    "jpserver",
    "jupyter",
    "jupyterlab",
    "labextension",
    "labextensions",
    "lintcache",
    "lumino",
    "monorepo",
    "nbformat",
    "pnpm",
    "pyproject",
    "Pytest",
    "pytest",
    "rseidelsohn",
    "sdist",
    "settingregistry",
    "stylelintcache",
    "testutils",
    "venv",
    "ydoc",
