{
  "version": "0.2",
  "ignorePaths": [
    "node_modules",
    "package-lock.json",
    "yarn.lock",
    "dist",
    "lib",
    "build",
    "coverage",
    ".vscode",
    ".git",
    "*.min.js",
    "*.min.css",
    "*.map",
    "jupyterlab_deepnote/labextension",
    "CHANGELOG.md"
